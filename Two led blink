#include<pic.h>
#define led1 RB0;
#define led2 RB1;

void delay(unsigned int delay)
{
	unsigned int i,j;
	for(i=0;i<delay;i++);
{ 
    for(j=0;j<100;j++);
	}
}
void main()
{
	TRISB=0x00;
    PORTB=0x00;
	
	while(1)
	{
		RB0=1;
		delay(5000);
		RB0=0;
		delay(5000);
        RB1=1;
        delay(5000);
        RB1=0;
        delay(5000);
		}
}
		
